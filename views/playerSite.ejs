              aTags[i].addEventListener('click', (event) => {
                event.preventDefault(); // Chặn sự kiện mặc định của trình duyệt
                // Thực hiện AJAX tại đây
                fetch(`/players?page=${i + 1}&nation=${value}${url}`)
